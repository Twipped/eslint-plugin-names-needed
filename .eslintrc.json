{
  "extends": [
    "@twipped/eslint-config/node-esm",
    "@twipped/eslint-config/jsdoc"
  ],
  "rules": {
    "import/extensions": ["error", {
      "ignorePackages": true,
      "pattern": {
        "js": "always",
        "mjs": "always",
        "jsx": "always"
      }
    }],
    "node/no-unsupported-features/es-syntax": "off"
  }
}